<UserControl x:Class="Celestrak.NORAD.Satellites.Wpf.UI.UserControls.UserControlCelestrak"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
            xmlns:local="clr-namespace:Celestrak.NORAD.Satellites.Wpf.UI.UserControls"             
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl>
            <TabItem Header="Element Sets">
                <local:UserControlCelestrakPage x:Name="Page"/>
            </TabItem>
            <TabItem Header="Group">
                <local:UserControlGroupPage x:Name="Group"/>
            </TabItem>
            <TabItem Header="Satellite">
                <local:UserControlSatellite x:Name="Satellite"/>
            </TabItem>

        </TabControl>
    </Grid>
</UserControl>
